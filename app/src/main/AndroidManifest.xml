<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.george.vector">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECIVE" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Palladium">

        <service
            android:name=".develop.notifications.FirebaseService"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
                <action android:name="com.google.c2dm.intent.RECIVE" />
            </intent-filter>
        </service>

        <activity android:name=".auth.LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".develop.notifications.NotificationActivity" />
        <activity android:name=".develop.DevelopActivity" />
        <activity android:name=".users.executor.tasks.TaskExecutorActivity" />
        <activity android:name=".users.executor.main.ExecutorTasksActivity" />
        <activity android:name=".users.executor.main.FolderExecutorActivity" />
        <activity android:name=".users.caretaker.tasks.EdtTaskCaretakerActivity" />
        <activity android:name=".users.caretaker.tasks.AddTaskCaretakerActivity" />
        <activity android:name=".users.caretaker.tasks.TaskCaretakerActivity" />
        <activity android:name=".users.caretaker.main.FolderCaretakerActivity" />
        <activity android:name=".users.caretaker.main.FolderTaskCaretakerActivity" />
        <activity android:name=".users.root.tasks.EditTaskActivity" />
        <activity android:name=".users.root.tasks.TaskRootActivity" />
        <activity android:name=".users.root.tasks.AddTaskRootActivity" />
        <activity android:name=".users.root.main.FolderRootActivity" />
        <activity android:name=".users.root.main.LocationFolderActivity" />
        <activity android:name=".users.caretaker.main.MainCaretakerActivity" />
        <activity android:name=".users.root.main.RootMainActivity" />
        <activity android:name=".common.edit_users.EditDataUserActivity" />
        <activity android:name=".common.edit_users.EditUserActivity" />
        <activity android:name=".common.edit_users.ListUsersActivity" />
        <activity android:name=".users.admin.tasks.EditTaskAdminActivity" />
        <activity android:name=".users.user.tasks.TaskUserActivity" />
        <activity android:name=".users.user.tasks.AddTaskUserActivity" />
        <activity android:name=".users.executor.main.MainExecutorActivity" />
        <activity android:name=".users.admin.tasks.TaskAdminActivity" />
        <activity android:name=".users.admin.tasks.AddTaskAdminActivity" />
        <activity android:name=".users.admin.main.FolderAdminActivity" />
        <activity android:name=".users.user.main.MainUserActivity" />
        <activity android:name=".auth.ActivityRegisterUser" />
        <activity android:name=".users.admin.main.MainAdminActivity" />
        <activity android:name=".auth.ActivityLogin" />



    </application>

</manifest>